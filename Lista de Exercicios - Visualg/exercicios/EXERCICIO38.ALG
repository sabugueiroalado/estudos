algoritmo "exercicio38"
var
   C: Inteiro
inicio
      C <- 6
      
      escrevaL("---------------------")
      escrevaL("  CONTAGEM 6 ATÉ 11  ")
      escrevaL("---------------------")
      escrevaL("")
      
      Enquanto (C <= 11) faca
               escreva(C)
               C <- C + 1
      FimEnquanto
      escrevaL("")
      escreva("Acabou!")

fimalgoritmo