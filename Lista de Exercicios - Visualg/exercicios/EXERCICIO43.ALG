algoritmo "exercicio43"
var
   C: Inteiro
inicio
      C <- 30
      Enquanto (C >= 1) faca
      
               Se (C % 4 = 0) entao
                  escreva(" [", C ," ]")
               FimSe
               
               escreva(C)
               C <- C - 1
      FimEnquanto
      
      escrevaL("")
      escreva("Acabou!")
fimalgoritmo