algoritmo "exercicio03"

var
   nome: caractere
   salario: real
inicio
      escreva("Nome do funcionário(a)? ")
      leia(nome)
      escreva("Salário do funcionário(a)? ")
      leia(salario)
      escreva("O funcionário(a), ", nome, " tem um salário de R$: ", salario, " em Fevereiro de 2025")
fimalgoritmo