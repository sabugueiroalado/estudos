algoritmo "exercicio11"
var
   A, B, C, D: Real
inicio
    escreva("Informe o valor de A: ")
    leia(A)
    escreva("Informe o valor de B: ")
    leia(B)
    escreva("Informe o valor de C: ")
    leia(C)
    
    D <- (B ^ 2) - (4 * (a * C))
    
    escreva("Delta vale: ", D)
fimalgoritmo